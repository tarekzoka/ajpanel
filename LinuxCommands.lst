------------------------------------->>>>>@@@@--- Command  اوامر الانيجما2 ---@@@@
opkg install /tmp/*.ipk
opkg install --force-overwrite /tmp/*.ipk
opkg install --force-reinstall /tmp/*.ipk
[Install tar.gz extension]
tar xzvpf /tmp/*.tar.gz -C /
[Install tar.gz extention 2]
tar -xzvf /tmp/*.tar.gz -C /
[WIFI 3070 intall from feed]
opkg update
opkg install rt3070
opkg install firmware-rt3070
[Full restart]
init 6
[Fast restart]
killall -9 enigma2
[Reboot(Fast restart)]
reboot
[Shutdown]
shutdown -h now
[Grab to media/hdd]
grab -d -p /media/hdd/grab.jpg
[Grab to tmp]
grab -d -p /tmp/grab.jpg
[Factory Reset]
rm -R /etc/enigma2 && killall -9 enigma2

@@ اوامر تحديث الصورة وتحديث الفيد@@

@@ Opensource @@
opkg update
opkg download FILE_NAME
opkg update; opkg upgrade
opkg flag hold FILE_NAME  # Stop Plug-in auto updating
opkg flag user FILE_NAME  # Star Plug-in auto updating
opkg install --force-overwrite /tmp/*.ipk
opkg remove --force-depends FILE_NAME
opkg remove --force-depends enigma2-plugin-softcams-ncam
opkg remove --force-depends enigma2-plugin-softcams-oscam*

@@ DreamOS @@
apt-get update
apt-get update; apt-get upgrade
apt-get install -f -y
dpkg -i --force-overwrite /tmp/*.deb
apt-get purge --auto-remove FILE_NAME
apt-get purge --auto-remove enigma2-plugin-softcams-ncam
apt-get purge --auto-remove enigma2-plugin-softcams-oscam*

